  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/votre-username/ia-webgen-pro.git"
  },
  "devDependencies": {
    "concurrently": "^7.6.0"
  },
  "engines": {
    "python": ">=3.8"
  }
}